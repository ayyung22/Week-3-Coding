function sumOfAll(arr) {
  let sum = 0;
  for (let a of arr) sum += a;
  return sum;
}
console.log(sumOfAll([5, 3, 4]));

function concat_q(word, n) {
  let word2 = "";
  for (let i = 0; i < 3; i++) {
    word2 = word2.concat(word);
  }
  return word2;
}
console.log(concat_q("Annie", 3));

function fullName(firstName, lastName) {
  return firstName + " " + lastName;
}
var fullname = fullName("Daisy", "Waters");
console.log(fullname);

let arrays = [12, 14, 16, 18, 20, 22];

function greaterThan100(array) {
  sum_num = 0;
  for (i = 0; i < array.length; i++) {
    sum_num = sum_num + array[i];
  }
  return sum_num > 100;
}

console.log(greaterThan100(arrays));

function averageNumbers(array) {
  sum_num = 0;
  for (i = 0; i < array.length; i++) {
    sum_num = sum_num + array[i];
  }

  return sum_num / array.length;
}

console.log(averageNumbers(array));

let arr1 = [20, 24, 26, 28];
let arr2 = [2, 4, 6, 8];

function averageOfNumbers(arr1) {
  sum_num = 0;
  for (i = 0; i < arr1.length; i++) {
    sum_num = sum_num + arr1[i];
  }
  return sum_num / arr1.length;
}
console.log(averageOfNumbers(arr1));

function averageOfNumbers2(arr2) {
  sum_num = 0;
  for (i = 0; i < arr2.length; i++) {
    sum_num = sum_num + arr2[i];
  }
  return sum_num / arr2.length;
}

console.log(averageOfNumbers(arr2));

function compare_average(arr1, arr2) {
  if (averageOfNumbers(arr1) > averageOfNumbers(arr2)) {
    return true;
  } else {
    return false;
  }
}

console.log(compare_average(arr1, arr2));

function willBuyDrink(hot, money) {
  if (hot == true && money > 10.5) {
    return true;
  } else {
    return false;
  }
}

console.log(willBuyDrink(true, 11));

