let ages = ["3", "9", "23", "64", "2", "8", "28", "93"];
let last = ages[ages.length - 1];
let first = ages[ages.length - ages.length];
let diff = last - first;
console.log(diff);

ages.push("821");
let lastAge = ages[ages.length - 1];
let firstAge = ages[ages.length - ages.length];
let difference = lastAge - firstAge;
console.log(difference);

for (let i = 0; i < ages.length; i++) {
  console.log(ages[i]);
}

var i,
  sum = 0;
for (i = 0; i < ages.length; i++) {
  sum = sum + ages[i];
}
console.log(sum / ages.length);

let names = ["Sam", "Tommy", "Tim", "Sally", "Buck", "Bob"];
for (let i = 0; i < names.length; i++) {
  console.log(names[i]);
}

sum = 0;
for (i = 0; i < names.length; i++) {
  sum = sum + names[i].length;
}
console.log(sum / names.length);

let con = names[0];
for (i = 1; i < names.length; i++) {
  con = con.concat(" ", names[i]);
}
console.log(con);

